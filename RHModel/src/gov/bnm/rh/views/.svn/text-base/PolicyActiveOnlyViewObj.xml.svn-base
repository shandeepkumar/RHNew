       Policy.approverId, 
       Policy.createdBy, 
       Policy.isActive, 
       Policy.isAmendmentRecord, 
       Policy.itemId, 
       Policy.keywords, 
       Policy.partitionTypeId, 
       Policy.policyId, 
       Policy.system, 
       Policy.updatedBy,
dbo.concatPartitionTypeSectors(Policy.itemId, Policy.partitionTypeId) as Sectors,
dbo.concatPolicyPartition(Policy.itemId, 'inst') as InstitutionTypes,
dbo.concatPolicyPartition(Policy.itemId, 'dept') as IssuingDepartment,
dbo.getPolicyAttachment(Policy.itemId,'PPDF') as AttachmentId
FROM dbo.Policy Policy]]>
