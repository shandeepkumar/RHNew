package gov.bnm.view.rh.common;

public class PageNavigation {

    private int docCount = 10;
    private int docStartCount = 1;
    private int totalCount = 0;
    private int fetchTotalCount;
    private String buttonId = "cb3";


    public PageNavigation() {
        super();
    }


    public void setDocCount(int docCount) {
        this.docCount = docCount;
    }

    public int getDocCount() {
        return docCount;
    }

    public void setDocStartCount(int docStartCount) {
        this.docStartCount = docStartCount;
    }

    public int getDocStartCount() {
        return docStartCount;
    }

    public void setTotalCount(int totalCount) {
        this.totalCount = totalCount;
    }

    public int getTotalCount() {
        return totalCount;
    }

    public void setButtonId(String buttonId) {
        this.buttonId = buttonId;
    }

    public String getButtonId() {
        return buttonId;
    }

    public void setFetchTotalCount(int fetchTotalCount) {
        this.fetchTotalCount = fetchTotalCount;
    }

    public int getFetchTotalCount() {
        return fetchTotalCount;
    }
}
